local keys = {
    "Key1",
    "Key2",
    "Key3"
}

local keyCheck

for i,v in pairs(keys) do
    if v == _G.Key then
        -- Whitelisted!
        keyCheck = _G.Key
        keys = ""
        break  -- Exit the loop after whitelisting
    end
end

while true do
    if _G.Key == keyCheck then
        -- Not spoofed
    else
        game.Players.LocalPlayer:Kick("Do not try and spoof your key!")
    end
    wait()
end
